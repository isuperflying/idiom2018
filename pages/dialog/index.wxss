/* components/dialog/index.wxss */

.container {
  position: absolute;
  left: 50%;
  top: 50%;
  width: 60%;
  min-height: 480rpx;
  border-radius: 20rpx;
  padding: 30rpx 20rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  transform: translateX(-50%) translateY(-50%);
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-image: url('data:image/png;base64,')
}

.c-title {
  height: 10%;
  color: #842800;
  font-size: 26px;
  font-weight: bold;
}

.c-content {
  display: flex;
  justify-content: center;
  align-items: center;
  color: #9a6424;
  font-size: 18px;
  height: 60%;
  margin: 10rpx;
}

.c-btn {
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-around;
  font-size: 26rpx;
}

.again-test{
  width: 180rpx;
}

.next-idiom{
  width: 180rpx;
}

.btn-success, .btn-retry {
  padding: 6rpx 30rpx;
  border-width: 4rpx;
  border-radius: 10rpx;
  color: #259f3d;
  display: flex;
  align-items: center;
  justify-content: center;
}

.btn-ignore {
  padding: 6rpx 24rpx;
  border-width: 4rpx;
  border-radius: 10rpx;
  color: white;
  display: flex;
  align-items: center;
  justify-content: center;
}

.btn-error {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-around;
  flex-direction: row;
}
